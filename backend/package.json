{
  "name": "otroski-racunalnik-backend",
  "version": "1.0.0",
  "description": "Backend API za interaktivno učenje računalnika",
  "main": "server.js",
  "scripts": {
  "start": "node server.js",
  "dev": "nodemon server.js",
  "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
  "test:watch": "NODE_ENV=test jest --watch",
  "seed": "npm run seed:all",
  "seed:lessons": "node seeds/seedLessons.js",
  "seed:achievements": "node seeds/seedAchievements.js",
  "seed:users": "node seeds/seedUsers.js",
  "seed:all": "node seeds/seedLessons.js && node seeds/seedAchievements.js && node seeds/seedUsers.js",
  "backup": "node scripts/backup.js",
  "test:services": "node test/testServices.js"

  },
  "keywords": [
    "mern",
    "education",
    "api"
  ],
  "author": "Skupina2",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongoose": "^8.19.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "jest": "^29.6.1",
    "supertest": "^6.3.3"
  }
}

